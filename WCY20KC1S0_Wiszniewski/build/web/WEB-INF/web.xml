<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>usun</servlet-name>
        <servlet-class>Servlets.usun</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>wyswietl</servlet-name>
        <servlet-class>Servlets.wyswietl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pobierz</servlet-name>
        <servlet-class>Servlets.pobierz</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>generuj</servlet-name>
        <servlet-class>Servlets.generuj</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>wstaw</servlet-name>
        <servlet-class>Servlets.wstaw</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>usun</servlet-name>
        <url-pattern>/usun</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>wyswietl</servlet-name>
        <url-pattern>/wyswietl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pobierz</servlet-name>
        <url-pattern>/pobierz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>generuj</servlet-name>
        <url-pattern>/generuj</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>wstaw</servlet-name>
        <url-pattern>/wstaw</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
